/* Formatted on 4/16/2021 8:06:36 AM (QP5 v5.256.13226.35510) */
SELECT cut_issue.system_id,
       cut_issue.color,
       cut_issue.wash,
       cut_issue.org_id,
       cut_issue.style_name,
       cut_issue.season,
       cut_issue.line_no, --cut_issue.WIP_ENTITY_ID,--cut_issue.WIP_ENTITY_NAME,
       NVL (OUTPUT_QTY, 0) BEL_QTY,
       NVL (CUT_ISS_QTY, 0) - NVL (OUTPUT_QTY, 0) - NVL (TOTAL_REJ, 0) WIP,
       NVL (CUT_ISS_QTY, 0) - NVL (OUTPUT_QTY, 0) ISS_QUANTITY,
       sam
  FROM (  SELECT MMC.SYSTEM_ID,
                 PL.COLOR,
                 PL.wash_name wash,
                 MMC.DIVISION_ID org_id,
                 PH.STYLE STYLE_NAME,
                 PH.SEASON,
                 LINE_NO,                                      --pl.wash_name,
                 --we.WIP_ENTITY_ID,
                 --WIP_ENTITY_NAME,
                 SUM (NVL (ISSUED_QUANTITY, 0)) CUT_ISS_QTY,
                 FIND_STITCH_LINEOUTPUT_LSTSAM (MMC.SYSTEM_ID) SAM
            FROM MNJ_MFG_CUTISSUANCE MMC,
                 MNJ_MFG_CUTISSUANCE_LINE MMCL,
                 (SELECT DISTINCT system_id,
                                  poc_id,
                                  wash_name,
                                  color
                    FROM xx_om_POC_L_t) PL,
                 xx_om_POC_H_t PH     --,WIP_DISCRETE_JOBS WDJ,wip_entities we
           WHERE     MMC.CUTISSUE_ID = MMCL.CUTISSUE_ID
                 --AND MMC.SYSTEM_ID =:P_SYSTEM_ID -- 4371
                 AND MMC.SYSTEM_ID = PL.system_id
                 AND pl.poc_id = ph.poc_id
                 AND mmc.issue_to = 'Issued to Stich'
                 AND mmc.attribute1 = (SELECT name
                                         FROM hr_operating_units
                                        WHERE ORGANIZATION_ID = :P_ORG_ID)
        --AND WDJ.CLASS_CODE = 'STI'
        --AND wdj.status_type NOT IN (7, 12)
        --AND TO_CHAR (PL.SYSTEM_ID) = WDJ.ATTRIBUTE10
        --and wdj.WIP_ENTITY_ID=we.WIP_ENTITY_ID
        --AND (SELECT-- DISTINCT
        --MMODA.FINAL_MFG_ORG_ID
        --FROM MNJ_MFG_OSP_DEP_ASSIGN MMODA,
        --MNJ_MFG_OSP_STN_LINE MMOSL,
        --MNJ_MFG_OSP_HEADER MMOH,xx_om_bpo_info_t bp,xx_om_poc_l_t pl1
        --WHERE MMODA.STN_LINE_ID = MMOSL.STN_LINE_ID
        --AND MMOSL.HEADER_ID = MMOH.HEADER_ID
        --and mmosl.bpo_id=bp.bpo_id
        --and bp.fob_id=pl1.fob_id
        --and pl1.system_id=pl.system_id
        ------AND TO_CHAR (MMOSL.SYSTEM_ID) = WDJ.ATTRIBUTE10
        --AND MMODA.FINAL_DEPARTMENT = 'Stitching'
        --AND ROWNUM = 1
        --) = :P_ORG_ID
        GROUP BY PH.STYLE,
                 PH.SEASON,
                 PL.COLOR,                                     --pl.wash_name,
                 MMC.ATTRIBUTE1,
                 --RATION_NO,
                 MMC.SYSTEM_ID,
                 LINE_NO,                                --we.WIP_ENTITY_NAME,
                 PL.wash_name,
                 MMC.DIVISION_ID                           --,we.WIP_ENTITY_ID
                                ) CUT_ISSUE,
       (  SELECT                                              --PROD_TRANS_ID,
                DIVISION_ID ORGANIZATION_ID,
                 SUBLINE_NO LINE_NO,
                 MMLS.SYSTEM_ID,
                 PH.style,
                 ph.season,
                 pl.wash_name,
                 pl.color,
                 SUM (NVL (LINE_OUTPUT, 0)) OUTPUT_QTY,
                 SUM (NVL (LINE_OUTPUT, 0)) TOTAL_REJ
            FROM MNJ_MFG_LINEOUTPUT_SBLINE MMLS,
                 (SELECT DISTINCT system_id,
                                  poc_id,
                                  wash_name,
                                  color
                    FROM xx_om_POC_L_t) PL,
                 xx_om_POC_H_t PH
           WHERE     1 = 1
                 AND MMLS.system_id = pl.system_id
                 AND PL.POC_ID = ph.poc_id
        --and MMLS.system_id=:P_SYSTEM_ID--4371
        GROUP BY DIVISION_ID,
                 SUBLINE_NO,
                 MMLS.SYSTEM_ID,
                 PH.style,
                 ph.season,
                 pl.wash_name,
                 pl.color) STI_OUT
 WHERE     1 = 1
       AND cut_issue.system_id = sti_out.system_id(+)
       AND cut_issue.line_no = sti_out.line_no(+)
       AND NVL (CUT_ISS_QTY, 0) - NVL (OUTPUT_QTY, 0) - NVL (TOTAL_REJ, 0) >
              0]]>
  </SQLQuery>
