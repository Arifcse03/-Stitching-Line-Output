  <SQLQuery>
    <![CDATA[SELECT  ooha.cust_po_number || '/' || oola.line_number STN,
           ooha.cust_po_number bpo_no,
       ooha.ORDER_NUMBER,
       ooha.HEADER_ID,
       ooha.org_id,
       ac.CUSTOMER_NAME,
       mmph.style_name,
       mmph.style_no,
       mmph.brand,
       mmph.season,
       mmph.fit,
       mmph.end_user,
       mmph.item_description,
       oola.ORDERED_ITEM,
       sum(oola.ordered_quantity) ordered_qty,
       ac.CUSTOMER_ID
  FROM oe_order_headers_all ooha,
       mnj_mfg_precosting_h mmph,
       oe_order_lines_all   oola,
       ar_customers         ac
 where ooha.attribute5 = to_char(mmph.header_id)
   and ooha.header_id = oola.header_id
   and ac.CUSTOMER_ID = ooha.SOLD_TO_ORG_ID
   /*and ac.CUSTOMER_ID = 7145*/
  /* and mmph.style_name = :p_style*/
 and ooha.HEADER_ID=:p_headerId
 AND ooha.org_id =:p_Org_Id
   /*and ooha.header_id not in
       (select h.saleorder_id
          from CUST_MNJ_ONT_BOM h
         where h.saleorder_id is not null)*/
 group by ooha.cust_po_number,
          ooha.ORDER_NUMBER,
          ooha.HEADER_ID,
          ooha.org_id,
          ac.CUSTOMER_NAME,
          mmph.style_name,
          mmph.style_no,
          mmph.brand,
          mmph.season,
          mmph.fit,
          mmph.end_user,
          mmph.item_description,
          ac.CUSTOMER_ID,
           ooha.cust_po_number || '/' || oola.line_number,
            oola.ORDERED_ITEM]]>
  </SQLQuery>
