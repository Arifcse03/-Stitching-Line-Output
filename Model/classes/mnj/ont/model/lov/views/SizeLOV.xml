  <SQLQuery>
    <![CDATA[SELECT 
        LINE_NO,
        USER_ITEM_DESCRIPTION ,
        MAIN_Q.STYLE,
        OC_NO,
        SUM(TOTAL_QTY) TOTAL_QTY,
        INSEAM_SIZE_CONCAT,
        INSEAM,
        SIZE_QTY ,
        NVL(ST.TOTAL_CUTQTY,0) BEL_QTY,
        NVL(ST.TOTAL_REJECTION,0) PREV_REJ_QTY,
        SUM(TOTAL_QTY) - (NVL(ST.TOTAL_CUTQTY,0) - NVL(ST.TOTAL_REJECTION,0))   WIP_QTY,
        BUYER_ID,
        MAIN_Q.SEASON,
        ST.COLOR
FROM 
(   
SELECT 
        MMC.LINE_NO,
        OOLA.USER_ITEM_DESCRIPTION ,
        MMC.STYLE,
        MMC.BUYER_ID,
        MMC.SEASON,
        MMC.COLOR,
        OOHA.CUST_PO_NUMBER || '/' || OOLA.LINE_NUMBER OC_NO,
               (   SELECT COUNT(*) 
              FROM MNJ_MFG_PIECE_STN_RELATION MMPS,
               (
                   SELECT 
                   MMCD.SRQ_NUMBER_FRM MINI, MMCD.SRQ_NUMBER_TO MAXI,MMCD.BANDEL_NO
                   FROM
                   MNJ_MFG_CUTISSUANCE_LINE     MMCL,
                   MNJ_MFG_CUTISSUANCE_SZLINE   MMCD
                   WHERE 1=1
                   AND MMC.CUTISSUE_ID          = MMCL.CUTISSUE_ID
                   AND MMCL.CUTISSUE_LINE_ID    = MMCD.CUTISSUE_LINE_ID
                   AND
                         NVL(( 
                            SELECT SUBSTR (MMCD.SIZ_BND_ID,
                                                               0,
                                                               INSTR (MMCD.SIZ_BND_ID,
                                                                      '~'
                                                                     )-1
                                                              )
                                                  FROM DUAL
                              ),MMCD.SIZ_BND_ID)         
                              = 
                                (
                                 SELECT MMRSV.SIZE1 
                                   FROM MNJ_MFG_RATIO_SIZE_V MMRSV 
                                  WHERE SIZE_D.INSEAM_SIZE_CONCAT    = MMRSV.SIZE_INSEAM
                                    AND ROWNUM = 1 
                                ) 
                   ) QUERY_M
             WHERE MMPS.PIECE_NO BETWEEN QUERY_M.MINI AND QUERY_M.MAXI
               AND MMPS.STN      = OOHA.CUST_PO_NUMBER || '/' || OOLA.LINE_NUMBER
               AND QUERY_M.BANDEL_NO = MMPS.BUNDLE_NO
               AND MMPS.INSEAM   =    (SELECT MMRSV.INSEAM 
                                         FROM MNJ_MFG_RATIO_SIZE_V MMRSV 
                                        WHERE SIZE_D.INSEAM_SIZE_CONCAT    = MMRSV.SIZE_INSEAM
                                          AND ROWNUM = 1 
                                        )
               AND MMPS.SIZEE    =    (
                                      SELECT MMRSV.SIZE1 
                                        FROM MNJ_MFG_RATIO_SIZE_V MMRSV 
                                       WHERE SIZE_D.INSEAM_SIZE_CONCAT    = MMRSV.SIZE_INSEAM
                                         AND ROWNUM = 1 
                                      )                         
               )  TOTAL_QTY,
        (SELECT MMRSV.SIZE1 
           FROM MNJ_MFG_RATIO_SIZE_V MMRSV 
          WHERE SIZE_D.INSEAM_SIZE_CONCAT    = MMRSV.SIZE_INSEAM
            AND ROWNUM = 1 
        ) INSEAM_SIZE_CONCAT,
   (SELECT MMRSV.INSEAM 
           FROM MNJ_MFG_RATIO_SIZE_V MMRSV 
          WHERE SIZE_D.INSEAM_SIZE_CONCAT    = MMRSV.SIZE_INSEAM
            AND ROWNUM = 1 
        )  INSEAM,
        SUM(ROUND(SIZE_D.SIZE_QTY)) SIZE_QTY,
        OOHA.CUST_PO_NUMBER,  
        OOHA.HEADER_ID,
        OOHA.ORG_ID
--        NVL(ST.TOTAL_CUTQTY,0) BEL_QTY,
--        NVL(ST.TOTAL_REJECTION,0) PREV_REJ_QTY,
--        NVL(ST.TOTAL_CUTQTY,0) - NVL(ST.TOTAL_REJECTION,0)  WIP_QTY_1
   FROM 
       OE_ORDER_HEADERS_ALL         OOHA,
       OE_ORDER_LINES_ALL           OOLA,
--       MNJ_MFG_PRECOSTING_H         MMPH,
       MNJ_MFG_CUTISSUANCE          MMC,
--       WIP_DISCRETE_JOBS_V          WDJ,
       CUST_MNJ_ONT_SO_OBINSLINE    SIZE_D,
       CUST_MNJ_ONT_SO_OBIN_SIZLINE SIZE_L,
       MNJ_COLOR_V                  MCV,
       MTL_SYSTEM_ITEMS_B           MSIB
 WHERE 1 = 1 
   AND SIZE_L.SO_OBINSLINE_ID       = SIZE_D.SO_OBINSLINE_ID
   AND SIZE_L.SALES_ORDER_ID        = OOHA.HEADER_ID
--   AND OOHA.ATTRIBUTE5          = TO_CHAR(MMPH.HEADER_ID)      
   AND MMC.BUYER_ID             = OOHA.SOLD_TO_ORG_ID
   AND MMC.STYLE                = OOHA.SHIPPING_INSTRUCTIONS
   AND MMC.SEASON               = OOHA.ATTRIBUTE4
   AND OOHA.HEADER_ID           = OOLA.HEADER_ID
--   AND OOHA.CUST_PO_NUMBER || '/' || OOLA.LINE_NUMBER = WDJ.ATTRIBUTE2 (+)
----   AND OOHA.HEADER_ID               = WDJ.ATTRIBUTE1 (+)
--   AND WDJ.ATTRIBUTE_CATEGORY (+)   = 'STITCHING'
   AND MMC.ISSUE_TO             = 'Issued to Stich'
   AND MMC.LINE_NO              IS NOT NULL
--   AND ST.BPO_NO         (+)   = OOHA.CUST_PO_NUMBER  
--   AND ST.SALE_ORDER_ID  (+)   = OOHA.HEADER_ID
--   AND ST.DIVISION_ID    (+)   = OOHA.ORG_ID
--   AND ST.SUBLINE_NO     (+)   = MMC.LINE_NO
   AND :P_HEADER_ID <> :P_LINE_ID --JUNK STATEMENT
   AND MSIB.INVENTORY_ITEM_ID   = OOLA.INVENTORY_ITEM_ID
   AND MSIB.ORGANIZATION_ID     = 353
   AND MCV.FLEX_VALUE           = MSIB.SEGMENT4
   AND MMC.COLOR                = MCV.VALUE_DESCRIPTION
    AND MMC.COLOR                = MCV.VALUE_DESCRIPTION
AND (SELECT DISTINCT MMODA.FINAL_MFG_ORG_ID
           FROM MNJ_MFG_OSP_DEP_ASSIGN MMODA,
                MNJ_MFG_OSP_STN_LINE MMOSL , 
                MNJ_MFG_OSP_HEADER MMOH 
          WHERE  MMOH.HEADER_ID = MMOSL.HEADER_ID
          and MMODA.STN_LINE_ID = MMOSL.STN_LINE_ID
            AND MMOSL.STN = OOHA.CUST_PO_NUMBER || '/' || OOLA.LINE_NUMBER
            AND MMOH.SEASON = OOHA.ATTRIBUTE4
            AND MMODA.FINAL_DEPARTMENT = 'Stitching') =:p_Org_Id
--   and OOHA.CUST_PO_NUMBER || '/' || OOLA.LINE_NUMBER = '522960-Hilltop-17/1'--'178716-5686/1'--
GROUP BY 
        MMC.LINE_NO,
        OOLA.USER_ITEM_DESCRIPTION ,
        MMC.STYLE,
        MMC.BUYER_ID,
        MMC.SEASON,
        OOHA.CUST_PO_NUMBER || '/' || OOLA.LINE_NUMBER ,
        SIZE_D.INSEAM_SIZE_CONCAT ,
        OOHA.CUST_PO_NUMBER,  
        OOHA.HEADER_ID,
        OOHA.ORG_ID,
        MMC.CUTISSUE_ID ,
        MMC.COLOR
) MAIN_Q ,
       (
           SELECT
            LL.BPO_NO,
            LL.SALE_ORDER_ID,
            LL.DIVISION_ID,
            LL.SUBLINE_NO,
            MAX(H.OUTREC_DATE)            PRODUCTION_DATE,   
            SUM(NVL(LL.DAY_REJECTION, 0)) TOTAL_DAY_REJECTION, 
            SUM(NVL(LL.LINE_OUTPUT,0))    TOTAL_LINE_OUTPUT,  
                SUM(NVL(DD.REJECTION, 0))   TOTAL_REJECTION,
                SUM(NVL(DD.CUTQTY, 0))      TOTAL_CUTQTY,
            DD.SIZE_VAL,
            DD.INSEEM ,             
            LL.COLOR  ,
            LL.SEASON  ,
            LL.STYLE
           FROM 
                MNJ_MFG_LINEOUTPUT          H, 
                MNJ_MFG_LINEOUTPUT_SBLINE   LL,
                MNJ_MFG_LINEOUTPUT_DETAIL   DD
           WHERE 
                H.PROD_TRANS_ID         = LL.PROD_TRANS_ID
             and LL.OUT_SUBLINE_ID           = DD.OUT_SUBLINE_ID
           GROUP BY             
            LL.BPO_NO,
            LL.SALE_ORDER_ID,
            LL.DIVISION_ID,
            LL.SUBLINE_NO  ,
            DD.SIZE_VAL,
            DD.INSEEM  ,           
            LL.COLOR  ,
            LL.SEASON  ,
            LL.STYLE 
       ) ST
WHERE MAIN_Q.TOTAL_QTY <> 0
  AND ST.BPO_NO         (+)   = MAIN_Q.CUST_PO_NUMBER  
  AND ST.SALE_ORDER_ID  (+)   = MAIN_Q.HEADER_ID
  AND ST.DIVISION_ID    (+)   = MAIN_Q.ORG_ID
  AND ST.SUBLINE_NO     (+)   = MAIN_Q.LINE_NO
  AND ST.SIZE_VAL       (+)   = MAIN_Q.INSEAM_SIZE_CONCAT    
  AND ST.INSEEM         (+)   = MAIN_Q.INSEAM        
  AND ST.COLOR          (+)   = MAIN_Q.COLOR 
  AND ST.SEASON         (+)   = MAIN_Q.SEASON
  AND ST.STYLE          (+)   = MAIN_Q.STYLE 
--  AND MAIN_Q.CUST_PO_NUMBER like 'DMN%MRTN-C4-17-1'
GROUP BY 
LINE_NO,
        USER_ITEM_DESCRIPTION ,
        MAIN_Q.STYLE,
        OC_NO,
        --TOTAL_QTY,
        INSEAM_SIZE_CONCAT,
        INSEAM,
        SIZE_QTY,
        NVL(ST.TOTAL_CUTQTY,0),
        NVL(ST.TOTAL_REJECTION,0),
        BUYER_ID,
        MAIN_Q.SEASON,
        ST.COLOR]]>
  </SQLQuery>
