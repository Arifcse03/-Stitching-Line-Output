<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ProdTypeNewVo"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="mnj.ont.model.lov.ProdTypeNewVoRowImpl"
  ComponentClass="mnj.ont.model.lov.ProdTypeNewVoImpl"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.ont.model.lov.ProdTypeNewVo_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="P_STYLE"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="P_SEASON"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="P_BUYER_ID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="P_COLOR"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[SELECT DISTINCT MMRPC.PRODUCTION_TYPE
              FROM MNJ_MFG_RATIOPLAN MMR,
                   MNJ_MFG_RATIOPLAN_PLYLINE MMRP,
                   MNJ_MFG_RATIOPLAN_PLYCFLINE MMRPC
             WHERE MMR.RATIOPLAN_ID = MMRP.RATIOPLAN_ID
               AND MMRPC.PLYLINE_ID = MMRP.PLYLINE_ID
               AND MMR.STYLE        = :P_STYLE
               AND MMR.SEASON       = :P_SEASON
               AND MMR.BUYER_ID     = :P_BUYER_ID
               AND MMR.COLOR        = :P_COLOR]]>
  </SQLQuery>
  <ViewAttribute
    Name="ProductionType"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PRODUCTION_TYPE"
    Expression="PRODUCTION_TYPE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
